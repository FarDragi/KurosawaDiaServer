{
    "name": "kurosawadiaserver",
    "version": "1.0.0",
    "description": "Server from KurosawaDia",
    "main": "src/index.ts",
    "scripts": {
        "watch": "nodemon -e * --watch src --exec npm start",
        "start": "ts-node -r tsconfig-paths/register .",
        "lint": "eslint . --ext .ts",
        "build": "tsc -p .",
        "migrate:create": "prisma migrate save --experimental",
        "migrate:up": "prisma migrate up --experimental",
        "prisma:gen": "prisma generate",
        "prisma:studio": "prisma studio"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FarDragi/KurosawaDiaServer.git"
    },
    "author": {
        "name": "Gabriel Paulucci",
        "email": "gabrielpauluccimunizmendes@gmail.com",
        "url": "https://github.com/Gabriel-Paulucci"
    },
    "private": "true",
    "bugs": {
        "url": "https://github.com/FarDragi/KurosawaDia/issues"
    },
    "homepage": "https://kurosawa.zuraaa.com",
    "devDependencies": {
        "@prisma/cli": "2.11.0",
        "@types/express": "^4.17.8",
        "@types/node": "^14.14.6",
        "@types/socket.io": "^2.1.11",
        "@typescript-eslint/eslint-plugin": "^4.6.0",
        "@typescript-eslint/parser": "^4.6.0",
        "eslint": "^7.12.1",
        "eslint-config-standard": "^16.0.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "nodemon": "^2.0.6"
    },
    "dependencies": {
        "@nestjs/common": "^7.4.4",
        "@nestjs/core": "^7.4.4",
        "@nestjs/platform-express": "^7.4.4",
        "@nestjs/platform-socket.io": "^7.4.4",
        "@nestjs/websockets": "^7.4.4",
        "@prisma/client": "2.11.0",
        "dotenv": "^8.2.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^6.6.3",
        "ts-node": "^9.0.0",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.0.5"
    },
    "engines": {
        "node": ">=14.15.0",
        "yarn": ">=1.22.x"
